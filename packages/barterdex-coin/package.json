  "homepage": "https://github.com/chainmakers/komodo-ui-mono-repository/tree/master/packages/barterdex-coin#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "./dist/index.js",
  "jsnext:main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chainmakers/komodo-ui-mono-repository.git"
  },
  "scripts": {
    "prepare": "npm run lint && npm run build",
    "build": "tsc",
