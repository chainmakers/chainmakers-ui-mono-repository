    "coverage": "tsc -p tsconfig.test.json --noEmit && jest --verbose --coverage",
    "clean": "rm -rf coverage/* && rm -rf dist/*"
  },
  "bugs": {
    "url": "https://github.com/chainmakers/komodo-ui-mono-repository/issues"
  },
  "devDependencies": {
    "jest": "24.9.0",
    "tslint": "5.20.0",
    "typescript": "3.6.4"
  }
