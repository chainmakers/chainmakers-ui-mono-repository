{
  "name": "barterdex-utilities",
  "version": "0.17.1",
  "description": "Utilities for working with Komodo UI Mono Repository",
  "author": "Hoang Nam <particle4dev@gmail.com> (particle4dev)",
  "homepage": "https://github.com/chainmakers/komodo-ui-mono-repository/tree/master/packages/barterdex-utilities#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "./dist/index.js",
  "jsnext:main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "private": true,
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chainmakers/komodo-ui-mono-repository.git"
  },
  "scripts": {
    "prepare": "npm run lint && npm run build",
    "build": "../../node_modules/.bin/tsc",
    "lint": "../../node_modules/.bin/tslint -c \"../../configs/tslint.json\" -p tsconfig.json src/*.ts",
    "test": "../../node_modules/.bin/tsc -p tsconfig.test.json --noEmit && ../../node_modules/.bin/jest",
    "coverage": "../../node_modules/.bin/tsc -p tsconfig.test.json --noEmit && ../../node_modules/.bin/jest --verbose --coverage",
    "clean": "rm -rf coverage/* && rm -rf dist/*"
  },
  "bugs": {
    "url": "https://github.com/chainmakers/komodo-ui-mono-repository/issues"
  },
  "dependencies": {
    "bigi": "^1.4.2",
    "bitgo-utxo-lib": "^1.5.0",
    "js-sha256": "^0.9.0"
  }
}
