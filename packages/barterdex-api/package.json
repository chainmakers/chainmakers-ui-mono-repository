{
  "name": "barterdex-api",
  "version": "0.13.0",
  "description": "Barterdex API for Node",
  "author": "Hoang Nam <particle4dev@gmail.com> (particle4dev)",
  "homepage": "https://github.com/particle4dev/atomicdex/tree/master/packages/barterdex-api#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "./dist/index.js",
  "jsnext:main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/particle4dev/atomicdex.git"
  },
  "scripts": {
    "prepare": "npm run lint && npm run build",
    "build": "../../node_modules/.bin/tsc",
    "lint": "../../node_modules/.bin/tslint -c \"../../configs/tslint.json\" -p tsconfig.json src/*.ts",
    "test": "../../node_modules/.bin/tsc -p tsconfig.test.json --noEmit && ../../node_modules/.bin/jest",
    "coverage": "../../node_modules/.bin/tsc -p tsconfig.test.json --noEmit && ../../node_modules/.bin/jest --verbose --coverage"
  },
  "bugs": {
    "url": "https://github.com/particle4dev/atomicdex/issues"
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "nock": "^10.0.6"
  }
}
