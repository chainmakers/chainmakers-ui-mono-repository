{
  "name": "komodo-ui-mono-repo",
  "productName": "Komodo UI mono repo",
  "version": "0.13.0",
  "private": true,
  "description": "> TODO: description",
  "scripts": {
    "lint": "cross-env NODE_ENV=development eslint --cache --format=pretty .",
    "lint-fix": "yarn --silent lint --fix; exit 0",
    "postinstall": "lerna exec -- npm install --package-lock-only && lerna run prepare",
    "mono-test": "lerna run test",
    "mono-prepare": "lerna run prepare",
    "mono-coverage": "lerna run coverage",
    "update-authors": "./bin/update-authors"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "cross-env NODE_ENV=development eslint --cache --format=pretty",
      "prettier --ignore-path .eslintignore --single-quote --write",
      "git add"
    ],
    "{*.json,.{babelrc,eslintrc,prettierrc,stylelintrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write",
      "git add"
    ],
    "*.{css,scss}": [
      "stylelint --ignore-path .eslintignore --syntax scss --fix",
      "prettier --ignore-path .eslintignore --single-quote --write",
      "git add"
    ],
    "*.{yml,md}": [
      "prettier --ignore-path .eslintignore --single-quote --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/particle4dev/atomicdex.git"
  },
  "author": {
    "name": "Hoang Nam",
